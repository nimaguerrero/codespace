{
  "name": "codespace-client",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve -o --configuration=development",
    "start:staging": "ng serve -o --configuration=staging",
    "nghm": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run mrstems:serve-ssr",
    "serve:ssr": "node dist/mrstems/server/main.js",
    "build:ssr": "ng build && ng run mrstems:server",
    "prerender": "ng run mrstems:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/platform-server": "~13.0.3",
    "@angular/router": "~13.0.3",
    "@auth0/angular-jwt": "^5.0.2",
    "@nguniversal/express-engine": "^13.0.1",
    "cross-env": "^7.0.3",
    "express": "^4.15.2",
    "moment": "^2.29.1",
    "ngx-stars": "^1.5.1",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.4.0",
    "sweetalert2": "^11.3.0",
    "timeago.js": "^4.0.2",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.2",
    "@types/node": "^16.11.14",
    "@types/uuid": "^8.3.3",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.2"
  }
}
